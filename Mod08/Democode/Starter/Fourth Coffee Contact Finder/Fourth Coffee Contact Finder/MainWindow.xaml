<Window x:Class="Fourth_Coffee_Contact_Finder.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Fourth Coffee Contact Finder" Height="216" Width="525">
    <Grid Margin="0,0,0,-1">
        <Label x:Name="name" HorizontalAlignment="Left" Margin="94,69,0,0" VerticalAlignment="Top" Height="28" Width="322"/>
        <Label Content="Name:" HorizontalAlignment="Left" Margin="50,71,0,0" VerticalAlignment="Top"/>
        <Label Content="Area:" HorizontalAlignment="Left" Margin="57,101,0,0" VerticalAlignment="Top"/>
        <Label x:Name="area" HorizontalAlignment="Left" Margin="94,101,0,0" VerticalAlignment="Top" Height="28" Width="322"/>
        <Label Content="Email Address:" HorizontalAlignment="Left" Margin="8,130,0,0" VerticalAlignment="Top"/>
        <Label x:Name="emailAddress" HorizontalAlignment="Left" Margin="94,130,0,0" VerticalAlignment="Top" Height="28" Width="322"/>
        <Button x:Name="go" Content="GO" HorizontalAlignment="Left" Margin="341,23,0,0" VerticalAlignment="Top" Width="75" Click="go_Click"/>
        <Label Content="Search:" HorizontalAlignment="Left" Margin="50,19,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="searchName" HorizontalAlignment="Left" Height="23" Margin="103,23,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="233"/>

    </Grid>
</Window>
